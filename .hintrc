{
  "extends": [
    "development"
  ],
  "hints": {
    "disown-opener": "off",
    "axe/text-alternatives": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "listitem": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "min-height: fit-content",
          "width: fit-content",
          "text-underline-offset",
          "height: fit-content"
        ]
      }
    ]
  }
}